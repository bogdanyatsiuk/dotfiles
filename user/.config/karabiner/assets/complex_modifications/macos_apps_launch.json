{
  "title": "Launch macOS apps",
  "rules": [
    {
      "description": "Launch apps by right command+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Alacritty.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Bitwarden.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Chrome.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open ~"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/iTerm.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Sublime Text.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Obsidian.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Safari.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Sourcetree.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Visual Studio Code.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Zotero.app'"
            }
          ]
        }
      ]
    }
  ]
}
